[Info]
name = "Code Red"       ;Nome do cenário.

[Camera]
startx = 0              ;Especifica em que local do eixo x inicia a câmera.
starty = 0              ;Especifica em que local do eixo y inicia a câmera.
boundleft = -220        ;Especifica o limite esquerdo do cenário.
boundright = 220        ;Especifica o limite direito do cenário.
boundhigh = -237        ;Especifica o limite do alto do cenário.
boundlow = 0            ;Especifica o limite de baixo do cenário.
verticalfollow = .5     ;Indica o quanto deve subir o cenário quanto o
                        ;lutador saltar orientando-se pelo mais alto.
tension = 60            ;Distância mínima entre os lutadores para que a câmera comece a mover.

;----------------------
[PlayerInfo]
 ;Coordenadas de inicialização dos lutadores.
p1startx = -82          ;Coordenadas x de inicialização
p1starty = 0            ;Coordenadas y de inicialização
p1startz = 0            ;Coordenadas z de inicialização
p1facing = 1            ;Direção da face do lutador: 1=dir, -1=esq
p2startx = 80
p2starty = 0
p2startz = 0
p2facing = -1

leftbound  = -1000      ;Limite esquerdo do movimento dos lutadores.
rightbound =  1000      ;Limite direito do movimento dos lutadores.
topbound  =  0          ;Limite do movimento z.
botbound  =  0          ;Limite do movimento para o fundo.

;----------------------
[Scaling]
;Escalas em geral (geralmente não é necessário mudar).
topz     = 0            ;Coordenadas de escala z do topo.
botz     = 50           ;Coordenadas de escala z do fundo.
topscale = 1            ;Escale para usar no topo .
botscale = 1.2          ;Escale para usar no fundo. 

;----------------------
[Bound]
screenleft = 15         ;Distância que limita o movimento do lutador no canto esquerdo.
screenright = 15        ;O mesmo que o de cima, mas no canto direto.

;----------------------
[StageInfo]
zoffset = 0        ;Regula a distância do pé do lutador ao chão (maior
                   ;valor = mais baixo, menor = mais alto).
zoffsetlink = 10   ;Faz com q os lutadores sigam objetos especificados como "drummy"
autoturn = 1       ;Faz com que os lutadores sempre fiquem de frente um para o outro.
resetBG = 0        ;Regula se as animações reiniciam nos próximos rounds (valores 0 ou 1).

;----------------------
[Shadow]
color = 92,92,92   ;Valores de gama da sombra (vermelho, verde, azul)
yscale = .3        ;Tamanho da sombra (valores positivos e negativos)
reflect = 0        ;Adiciona reflexo ao lutador (1 ou 0).

;----------------------
[Music]
 ;Som ou música do cenário.
bgmusic = sound/looped.mp3     ;endereço do arquivo da música
bgvolume = 255               ;0 = normal, >0 = aumentar, <0 = diminuir (só mp3, cda e mod)

;----------------------
[BGdef] 
spr = stages/code_red.sff        ;Indica o local do arquivo dos sprits.
debugbg = 0                      ;0 = mancha o fundo com o movimento, 1 = fundo magenta.


