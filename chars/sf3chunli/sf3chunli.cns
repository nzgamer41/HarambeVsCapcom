[State 40, HJumpVarSet]
type = VarSet
trigger1 = Time = 0 || Command = "Longjump"
fvar(1) = Ifelse (Command = "Longjump", 1.5, 1)

[State 40, Velocity]
type = VelSet
trigger1 = AnimTime = 0
x = ((Ifelse(Sysvar(1)=0, Const(Velocity.Jump.Neu.X), Ifelse(Sysvar(1)=1, Const(Velocity.Jump.Fwd.X), Const(Velocity.Jump.Back.X))))*FVar(1))
y = Const(Velocity.Jump.Y)

[State 40, StateChange]
type = ChangeState
trigger1 = AnimTime = 0
value = 50
ctrl = 1

