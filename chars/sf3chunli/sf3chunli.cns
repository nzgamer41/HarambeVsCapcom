;점프 개시
[Statedef 40]
type = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1

[State 40, SysVarSet]
type = VarSet
trigger1 = Time = 0 || Command = "holdfwd" || Command = "holdback"
